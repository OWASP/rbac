class PHPUnit_Framework_Warning extends PHPUnit_Framework_TestCase
{
    /**
     * @var string
     */
    protected $message = '';

    /**
     * @var boolean
     */
    protected $backupGlobals = FALSE;

    /**
     * @var boolean
     */
    protected $backupStaticAttributes = FALSE;

    /**
     * @var boolean
     */
    protected $runTestInSeparateProcess = FALSE;

    /**
     * @var boolean
