                PHPUnit_Framework_Constraint_JsonMatches_ErrorMessageProvider::determineJsonError(
                    $jsonError,
                    PHPUnit_Framework_Constraint_JsonMatches_ErrorMessageProvider::translateTypeToPrefix('expected')
                );
